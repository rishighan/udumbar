<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="0">
	<defaultcontrol always="true">300</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<backgroundcolor>0xFF3F3F3F</backgroundcolor>
	<controls>

		<include>GlobalBackground</include>


			<control type="wraplist" id="50">
				<onup>1</onup>
				<ondown>2</ondown>
				<onleft>1</onleft>
				<onright>1</onright>
				<orientation>vertical</orientation>
				<pagecontrol>25</pagecontrol>
				<height>90</height>
				<itemlayout width="650" height="90">
			      <viewtype label="3D list">list</viewtype>
				<control type="label">
					<posx>80</posx>
					<posy>-</posy>
					<height>75</height>
					<width>375</width>
					<font>Thin</font>
					<textcolor>white</textcolor>
					<label>ListItem.label</label>
					<align>left</align>
				</control>

				<control type="image">
					<posx>80</posx>
					<posy>30</posy>
					<width>90</width>
					<height>90</height>
					<visible>true</visible>
					<texture>icons/earphones.png</texture>
				</control>

				</itemlayout>

				<focusedlayout width="650" height="90">
				<control type="label">
					<posx>80</posx>
					<posy>-</posy>
					<height>75</height>
					<width>375</width>
					<font>Thin</font>
					<textcolor>red</textcolor>
					<label>ListItem.label</label>
					<align>left</align>
				</control>

				<control type="image">
					<posx>80</posx>
					<posy>30</posy>
					<width>200</width>
					<height>200</height>

					<texture>icons/earphones.png</texture>
				</control>
				</focusedlayout>




			</control>

				<control type="label">
					<posx>200</posx>
					<posy>40</posy>
					<height>75</height>
					<width>375</width>
					<font>Thin</font>
					<label>MOVIES</label>
					<align>left</align>
				</control>

				<control type="image">
					<posx>80</posx>
					<posy>30</posy>
					<width>100</width>
					<height>100</height>

					<texture>icons/popcorn.png</texture>
				</control>



				<control type="label">
					<posx>200</posx>
					<posy>160</posy>
					<height>75</height>
					<width>375</width>
					<font>Thin</font>

					<label>MUSIC</label>
					<align>left</align>
				</control>

				<control type="image">
					<posx>80</posx>
					<posy>160</posy>
					<width>100</width>
					<height>100</height>
					<visible>true</visible>
					<texture>icons/earphones.png</texture>
				</control>


		<control type="rss">
			<description>RSS Feed</description>
			<posx>0</posx>
			<posy>1035</posy>
			<width>1920</width>
			<height>21</height>
			<font>Small</font>
			<urlset>1</urlset>
			<titlecolor>Red</titlecolor>
			<headlinecolor>White</headlinecolor>
			<textcolor>White</textcolor>
		</control>

	</controls>

</window>
