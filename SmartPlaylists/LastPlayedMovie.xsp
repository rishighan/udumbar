<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Movies watched in the last 20 days</name>
    <match>all</match>
    <rule field="lastplayed" operator="inthelast">
        <value>2 days</value>
    </rule>

    <limit>4</limit>
    <order direction="descending">lastplayed</order>
</smartplaylist>
